main :: () {
    a := !false && true;
    b := !(2 <= 3 && 2 >= 4 || 5 == 6 && 7 < 3 && 5 > 9);
    c := 5 != 2;
    d := false && true || 3 == 2;
    e := false && true;
    f := 2.5 + 1 > 4 || 2.0 * 3 == 6.0;

    print(a); // true
    print(b); // true
    print(c); // true
    print(d); // false
    print(e); // false
    print(f); // true
}