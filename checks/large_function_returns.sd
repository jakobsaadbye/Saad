Vector2 :: struct {
	x: float;
	y: float;
}

Vector3 :: struct {
	x: float;
	y: float;
	z: float;
}

Vector4 :: struct {
	x: float;
	y: float;
	z: float;
	w: float;
}

EntityKind :: enum {
	Monster,
	Human,
}

Entity :: struct {
	kind:		 EntityKind;
	position:    Vector3;
	orientation: Vector3;
	color: 		 Vector3;
}

large_return :: () -> Vector4 {
	r := Vector4{1, 4, 9, 11};
	return r;
}

small_return :: () -> Vector2 {
	r := Vector2{420, 69};
	return r;
}

args_and_large_return_value :: (x: float, y: float, z: float) -> Vector3 {
	r := Vector3{x, y, z};
	return r;
}

main :: () {
	v1 := large_return();
	print("%", v1);

	v2 := small_return();
	print("%", v2);

	v3 := args_and_large_return_value(27, 11, 98);
	print("%", v3);
}