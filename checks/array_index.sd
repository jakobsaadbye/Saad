Vector2 :: struct {
    x: float;
    y: float;
}

B :: struct {
    c: Vector2;
}

A :: struct {
    b: [2]B;
}

main :: () {
    a := [A{
        [B{{1, 3}}, {{4, 9}}]
    }];

    assert(a[0].b[0].c.x == 1);
    assert(a[0].b[0].c.y == 3);
    assert(a[0].b[1].c.x == 4);
    assert(a[0].b[1].c.y == 9);

    a[0].b[0].c.x = 1.5;
    a[0].b[0].c.y = 3.0;
    a[0].b[1].c.x = 4.5;
    a[0].b[1].c.y = 6.0;

    assert(a[0].b[0].c.x == 1.5);
    assert(a[0].b[0].c.y == 3.0);
    assert(a[0].b[1].c.x == 4.5);
    assert(a[0].b[1].c.y == 6.0);
}