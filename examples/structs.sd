EngineType :: enum {
    V8,
    V6,
}

DEFAULT_ENGINE_TYPE :: EngineType.V6;

Fuel :: enum {
    electric,
    diesel,
    gasoline,
}

Engine :: struct {
    type : EngineType;
    fuel : Fuel;
    cylinders : int;
    capacity : float;
}

Car :: struct {
    doors : int;
    max_speed : float;
    engine : Engine;
}

main :: () {
    bugatti := Car.{
        doors = 4,
        max_speed = 320,
        engine = .{
            type = DEFAULT_ENGINE_TYPE,
            fuel = .gasoline,
            cylinders = 8,
            capacity = 30
        }
    };

    print("%", bugatti.engine.type);
    print("%", bugatti.engine.fuel);
}