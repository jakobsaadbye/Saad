main :: () {

    // Exclusive range
    for 0..10 {
        print(it);
    }

    print(""); print("");

    // Inclusive range
    for 0...10 {
        print(it);
    }

    print(""); print("");

    // Ranges can be arbitrary expressions
    a := 0;
    b := 10;
    for a...b+10 {
        print(it);
    }

    print(""); print("");

    // Named for-loops
    for i in 0..10 {
        print(i);
    }

    print(""); print("");

    // Named inclusive for-loops with range expressions
    for i in a...b-5 {
        print(i);
    }
}