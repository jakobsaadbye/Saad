Vector2 :: struct {
    x: float;
    y: float;
}

Brick :: struct {
    x:     int;
    y:     int;
    dead:  bool;
}

main :: () {

    bricks: [9]Brick;

    for row in 0..3 {
        for col in 0..3 {
            index := (row * 3) + col;
            bricks[index] = Brick{x = col, y = row, dead = false};
        }
    }

    for bricks {
        print("%", it);
    }
}