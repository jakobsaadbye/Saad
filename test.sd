Vector2 :: struct {
    x: float;
    y: float;
}

Vector3 :: struct {
    x: float;
    y: float;
    z: float;
}

Entity :: struct {
    position: Vector2;
}

GameState :: struct {
    entities: []Vector2;
}

print_ints :: (nums: []int) {
    for n, i in nums {
        print("% = %", i, n);
    }
}

roll_random_dice :: (amount: int) -> []int {
    roll: [..]int = [3, 27, 1, 4, 5];
    return roll;
}

main :: () {
    roll := roll_random_dice(6);

    print_ints(roll);
}