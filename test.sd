// floor :: (value: f64) -> f64 #extern "C"

// PI  :: 3.1415929;
// TAU :: PI * 2;

foo :: (n: f64) -> f64 {
    return n*n;
}

main :: () {

    x : f32 = 1;
    y : f64 = 1;
    z : f32 = xx (x + y);

    typeof(foo(xx z));
    typeof(z);
    print("%", z);
}

