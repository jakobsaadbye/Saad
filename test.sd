Color :: struct {
    r: u8;
    g: u8;
    b: u8;
    a: u8;
}

Vector2 :: struct {
    x: float;
    y: float;
}

Particle :: struct {
    pos: Vector2;
    vel: Vector2;
    color: Color;
    size: int;
    lifetime: int;
}

ParticleEmitter :: struct {
    particles: [25]Particle;
    pos: Vector2;
    vel: Vector2;
    radius: float;
    color: Color;
    particleLifetime: int;
    isActive: bool;
}

main :: () {
    // particleEmitters: [8]ParticleEmitter;

    xs := [1, 2, 3, 4, 5];
    for *x in xs {
        x *= 2;
    }

    for x in xs {
        print("%", x);
    }
}