Color :: struct {
    r: u8;
    g: u8;
    b: u8;
    a: u8;
}

Vector2 :: struct {
    x: float;
    y: float;
}

Particle :: struct {
    pos: Vector2;
    vel: Vector2;
    color: Color;
    size: int;
    lifetime: int;
}

ParticleEmitter :: struct {
    particles: [25]Particle;
    pos: Vector2;
    vel: Vector2;
    radius: float;
    color: Color;
    particleLifetime: int;
    isActive: bool;
}

main :: () {
    particleEmitters: [8]ParticleEmitter;

    for pe, i in particleEmitters {
        print("%", typeof(pe));
        pe.color = {xx i, xx i, xx i, xx i};
        pe.particleLifetime = i;
    }

    for pe in particleEmitters {
        print("%", pe);
    }
}