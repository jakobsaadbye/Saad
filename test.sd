Vector2 :: struct {
    x: float;
    y: float;
}

Vector3 :: struct {
    x: float;
    y: float;
    z: float;
}

Entity :: struct {
    position: Vector2;
}

GameState :: struct {
    entities: []Vector2;
}

print_ints :: (nums: []int) {
    print("%", typeof(nums));
    for nums {
        print("%", it);
    }
}

main :: () {
    nums: [..]int = [1, 2, 3];

    print_ints(nums);
}