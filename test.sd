Vector2 :: struct {
	x: float;
	y: float;
}

Vector3 :: struct {
	x: float;
	y: float;
	z: float;
}

Vector4 :: struct {
	x: float;
	y: float;
	z: float;
	w: float;
}

// Where do struct literals appear?
// - initialization,  a := Vector2{1, 2}
// - returns,         return Vector2{3, 4};

foo :: () -> Vector3 {
	return {1, 4, 9};
}

main :: () {
	a := foo().x + foo().y + foo().z;
	print("%", a);
}